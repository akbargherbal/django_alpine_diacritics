{
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "entity.name.function.js",
          "meta.function-call.js"
        ],
        "settings": {
          "foreground": "#BA55D3",
          "fontStyle": "italic"
        }
      },
      {
        "scope": "entity.name.function.js",
        "when": "meta.function-call.js && entity.name.function.js == 'addEventListener'",
        "settings": {
          "foreground": "#FFD700",
          "fontStyle": "bold"
        }
      },
      {
        "scope": "entity.name.function.js",
        "when": "meta.function-call.js && entity.name.function.js == 'log'",
        "settings": {
          "foreground": "#00FF7F",
          "fontStyle": "italic"
        }
      },
      {
        "scope": "keyword.control.export.js",
        "settings": {
          "foreground": "#FF7F50",
          "fontStyle": "bold"
        }
      },
      {
        "scope": "keyword.control.js",
        "settings": {
          "foreground": "#00BFFF",
          "fontStyle": "italic"
        }
      },
      {
        "scope": "variable.other.js",
        "settings": {
          "foreground": "#F0E68C",
          "fontStyle": "normal"
        }
      },
      {
        "scope": "variable.other.object.js",
        "settings": {
          "foreground": "#3CB371",
          "fontStyle": "normal"
        }
      },
      {
        "scope": "meta.function.js",
        "settings": {
          "foreground": "#DDA0DD",
          "fontStyle": "bold"
        }
      },
      {
        "scope": "constant.numeric.js",
        "settings": {
          "foreground": "#32CD32",
          "fontStyle": "normal"
        }
      },
      {
        "scope": "string.quoted.double.js",
        "settings": {
          "foreground": "#E9967A",
          "fontStyle": "normal"
        }
      },
      {
        "scope": "string.quoted.single.js",
        "settings": {
          "foreground": "#FF7F50",
          "fontStyle": "normal"
        }
      },
      {
        "scope": "comment.line.double-slash.js",
        "settings": {
          "foreground": "#808080",
          "fontStyle": "italic"
        }
      },
      {
        "scope": "comment.block.js",
        "settings": {
          "foreground": "#2F4F4F",
          "fontStyle": "italic"
        }
      },
      {
        "scope": "support.class.js",
        "settings": {
          "foreground": "#DC143C",
          "fontStyle": "bold"
        }
      },
      {
        "scope": "support.type.js",
        "settings": {
          "foreground": "#1E90FF",
          "fontStyle": "normal"
        }
      }
    ]
  },
  "editor.semanticTokenColorCustomizations": {
    "rules": {
      "method.defaultLibrary": {
        "foreground": "#00FF7F",
        "fontStyle": "italic"
      }
    }
  }
}